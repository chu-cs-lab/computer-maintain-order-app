

.menu-item {
  background-color: rgb(255, 236, 196);
  border-left: 5px solid rgb(255, 199, 79);
  padding: 10px;
  margin-bottom: 10px;
  /* border-bottom: 2px inset rgba(240, 206, 12, 0.24); */
}
.menu-item::after{
  content: '';
  display: inline-block;
  width: 15px;
  height: 15px;
  float: right;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.face-wrap {
  margin: 50rpx 0 80rpx 0;
  text-align: center;
}
.face-img {
  
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}

.but-login {
  background-color: #FF6146;
  color: #fff;
  margin: 80rpx 10rpx;
}

.order-wrap {
  display: flex;
  flex-direction: column;
  margin: 20rpx 20rpx;
  padding:10px;
  border-radius: 5px;
  background-color: #f3d9d4;
}
.order-title{
  font-size: 1.1rem;
  padding: 5px;
  border-bottom: 1px solid rgba(192, 192, 192, 0.726);
}
.type-img {
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 20rpx;
}
.order-btn-group{
  margin-top: 20px;
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: space-evenly;
}
.order-btn-group navigator{
  text-align:center;
}
.link-wrap{
  padding: 10px;
}
.link-title{
  font-size: 1.1rem;
  padding: 5px;
}